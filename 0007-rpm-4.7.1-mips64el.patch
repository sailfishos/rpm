From fd7f2e7deb74b68b18dafa0dc92f50d1f99bcb76 Mon Sep 17 00:00:00 2001
From: David Greaves <david@dgreaves.com>
Date: Mon, 25 Jun 2012 23:57:04 +0100
Subject: [PATCH 07/19] rpm-4.7.1-mips64el

---
 installplatform |    6 ++++++
 1 files changed, 6 insertions(+), 0 deletions(-)

diff --git a/installplatform b/installplatform
index a1128ba..42fab41 100755
--- a/installplatform
+++ b/installplatform
@@ -51,6 +51,7 @@ for SUBST in $SUBSTS ; do
     s390x-linux) LIB=lib64 ;;
     ppc64-linux|powerpc64-linux) LIB=lib64 ;;
     x86_64-linux|amd64-linux|ia32e-linux) LIB=lib64 ;;
+    mips*-linux) LIB=lib ;;
     *) LIB=lib;;
   esac
 
@@ -107,6 +108,11 @@ for SUBST in $SUBSTS ; do
 	ISANAME=alpha
 	ISABITS=64
 	;;
+    mips*)
+	# We assume N32 first
+	ISANAME=mips
+	ISABITS=32
+	;;
   esac
 
   case $VENDOR in
-- 
1.7.9

